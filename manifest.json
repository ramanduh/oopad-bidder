{
  "manifest_version": 2,

  "name": "OOpad Bid Watcher",
  "description": "Extension for automatically bidding on the OOpad site",
  "version": "1.0",

  "browser_action": {
    "default_icon": "bid.png"
  },
  "content_scripts": [
	{
		"matches": ["http://fr.oopad.com/*/?DId=*"],
		"js": ["script.js"]
	}
  ],
  "permissions": [
    "activeTab",
	"tabs"
  ],
  "web_accessible_resources": [
	  "autobid.js"
  ]
}
